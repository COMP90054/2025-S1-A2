(define (animation tramNetwork)
; Modified from Grid Animation Profile https://github.com/planimation/documentation/blob/master/AnimationProfiles/Grid/grid_AP.pddl

; Specifies that two objects are connected
; We just draw a line between them
   (:predicate track-connected
               :parameters (?x ?y)
               :effect(
               (action (function draw_line (objects ?x ?y)))
               )
  )


; Specifies that an object is a place (track)
; Here we just distribute the objects in a grid formation on screen
; This distribute function automatically aligns objects on-screen based on any
; numbers detected in the objects' name. Hence we require that nodes are named according
; to the convention track0-0, track2-1, etc, or similar.
  (:predicate is-track
               :parameters (?p)
               :effect(
               (assign (?p x y) (function distribute_grid_around_point (objects ?p)))
               )
  )
  
  (:predicate tram-located-at
               :parameters (?r ?x)
               :effect(
               (assign (?r x y) (function distribute_within_objects_vertical (objects ?r ?x)(settings (spacebtw 20))))
               )
  )

    (:predicate at-stram
               :parameters (?x)
               :custom stram
               :effect(
               (equal (stram x) (add (?x x) 20))
               (equal (stram y) (add (?x y) 20))
               )
  )
  
  ; Tram visualisation from https://iconscout.com/free-icon/tram-on-road-train-railway-emoj-symbol_30782
  (:visual stram
            :type custom
            :objects stram
            :properties(
              (prefabImage img-tram)
              (showName FALSE)
              (x Null)
              (y Null)
              (color WHITE)
              (width 40)
              (height 40)
              (depth 2)
            )
  )

  (:visual tram
    :type predefine
    :objects (%tram)
    :properties(
          (prefabImage img-triangle)
          (showName FALSE)
          (x Null)
          (y Null)
          (color RANDOMCOLOR)
          (width 40)
          (height 40)
          (depth 2)
      )
  )

; Default track type. Circle shape by default.
  (:visual track
            :type default
            :properties(
              (prefabImage img-square)
              (showName FALSE)
              (x Null)
              (y Null)
              (color RANDOMCOLOR)
              (width 80)
              (height 80)
              (depth 1)
          )
  )
 (:image (img-circle )
          (img-diamond )
          (img-triangle )
          (img-square )
          (img-tram )
          (line  )
 )
)